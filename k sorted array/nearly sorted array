#include<bits/stdc++.h>
using namespace std;

class Solution
{
    public:
    
    //Function to return the sorted array.
   vector<int> nearlySorted(int arr[], int num, int k)
{
    vector<int> h; // Declare the vector here instead

    priority_queue<int, vector<int>, greater<int>> p;
    for (int i = 0; i < num; i++)
    {
        p.push(arr[i]);
        if (p.size() > k)
        {
            int l = p.top();
        
            p.pop();
            h.push_back(l);
        }
    }

    while (!p.empty())
    {
        int l = p.top();
        p.pop();
        h.push_back(l);
    }

    return h;
}

};



int main()
 {
	int T;
	cin>> T;
	
	while (T--)
	{
	    int num, K;
	    cin>>num>>K;
	    
	    int arr[num];
	    for(int i = 0; i<num; ++i){
	        cin>>arr[i];
	    }
	    Solution ob;
	    vector <int> res = ob.nearlySorted(arr, num, K);
	    for (int i = 0; i < res.size (); i++)
	        cout << res[i] << " ";
	        
	    cout<<endl;
	}


	
	return 0;
}

INPUT - 7 3
6 5 3 2 8 10 9
OUTPUT - 2 3 5 6 8 9 10
